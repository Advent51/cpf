<?xml version="1.0" encoding="UTF-8"?>
<cpk>
    <elementTypes>
        <elementType name="Dashboard" class="pt.webdetails.cpk.elements.impl.DashboardElementType">
            <elementLocations>
                <elementLocation path="dashboards/" isRecursive="true" pattern="^(?!admin).*\.wcdf"/>
                <elementLocation path="dashboards/admin" isRecursive="true" pattern=".*\.wcdf" adminOnly="true"/>
            </elementLocations>
        </elementType>
        <elementType name="Kettle" class="pt.webdetails.cpk.elements.impl.KettleElementType">
            <elementLocations>
                <elementLocation path="endpoints/kettle/" isRecursive="true" pattern=".*\.ktr"/>
                <elementLocation path="endpoints/kettle/" isRecursive="true" pattern=".*\.kjb"/>
            </elementLocations>
        </elementType>
    </elementTypes> 
</cpk> 
